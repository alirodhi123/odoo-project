<odoo>
    <data>
        <record id="dataform_product_template_form_view" model="ir.ui.view">
            <field name="inherit_id" ref="project.view_task_form2"/>
            <field name="model">project.task</field>
            <field name="arch" type="xml">
                <field name="user_id" position="before">
                    <field name="create_uid" readonly="True"/>
                </field>
                <field name="project_id" position="before">
                    <field name="x_name" readonly="True"></field>
                </field>
                <page name="description_page" position="after">
                    <page name="action_task" string="Analysis and Action">
                        <field name="x_project_task_line">
                            <tree editable="bottom">
                                <field name="x_category_problem"/>
                                <field name="x_akar_masalah"/>
                                <field name="x_task"/>
                                <field name="x_date_task"/>
                                <field name="x_users"/>
                                <field name="x_date_finish"/>
                                <field name="x_attch_file"/>
                                <field name="x_status"/>
                            </tree>
                        </field>
                    </page>
                </page>
                <!--<page name="extra_info"  position="replace" attrs="{'invisible': 'True'}" />-->
            </field>
        </record>
    </data>
</odoo>