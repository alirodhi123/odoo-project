<odoo>
    <data>

        <record id="seq_form_ticket" model="ir.sequence">
          <field name="name">Sequence for new ticket</field>
          <field name="code">seq.ticket</field>
          <field name="prefix">TIK-%(y)s%(month)s%(day)s-</field>
          <field name="padding">5</field>
          <field name="company_id" eval="False"/>
         </record>

    </data>
</odoo>