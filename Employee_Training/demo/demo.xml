       <record id="mt_course_name" model="mail.message.subtype">
            <field name="name">Course</field>
            <field name="res_model">training.training</field>
            <field name="default" eval="True"/>
            <field name="description">Course Is</field>
        </record>
        <record id="mt_price_id" model="mail.message.subtype">
            <field name="name">Price</field>
            <field name="res_model">training.training</field>
