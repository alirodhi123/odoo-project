<?xml version="1.0" encoding="utf-8"?>
  <openerp>
     <data noupdate="1">

        <record id="seq_form_kuitansi" model="ir.sequence">
          <field name="name">Sequence for new kuitansi</field>
          <field name="code">seq.kuitansi</field>
          <field name="prefix">KWT-%(y)s%(month)s%(day)s-</field>
          <field name="padding">5</field>
          <field name="company_id" eval="False"/>
         </record>

         <!-- Sequence Custom Register Payment -->
        <record id="seq_draft_register_payment" model="ir.sequence">
          <field name="name">Sequence for new Custom Register Payment</field>
          <field name="code">sequence.x.regis.payment.draft</field>
          <field name="prefix">PRE-%(y)s%(month)s%(day)s-</field>
          <field name="padding">5</field>
        </record>

     </data>
  </openerp>