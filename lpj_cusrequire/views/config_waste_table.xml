<odoo>
    <data>

        <record model="ir.ui.view" id="waste_table_tree">
            <field name="name">x.waste.table.tree</field>
            <field name="model">x.waste.table.precost</field>
            <field name="arch" type="xml">

                <tree>
                    <field name="name"/>
                    <field name="x_batas_atas"/>
                    <field name="x_batas_bawah"/>
                    <field name="x_waste_config"/>
                    <!--<field name="x_kategori_1"/>-->
                    <!--<field name="x_kategori_2"/>-->
                    <!--<field name="x_kategori_3"/>-->
                </tree>
            </field>
        </record>

         <record model="ir.ui.view" id="waste_table_form">
            <field name="name">x.waste.table.form</field>
            <field name="model">x.waste.table.precost</field>
            <field name="arch" type="xml">
                <form>

                        <group>
                            <field name="name"/>
                            <field name="x_batas_atas"/>
                            <field name="x_batas_bawah"/>
                            <field name="x_waste_config"/>
                            <!--<field name="x_kategori_1"/>-->
                            <!--<field name="x_kategori_2"/>-->
                            <!--<field name="x_kategori_3"/>-->
                            <field name="x_kategori">
                                <tree editable="bottom">
                                    <field name="x_waste_id" domain="[('id','=',parent.id)]" invisible="1"></field>
                                    <field name="x_kategori_id"></field>
                                    <field name="x_harga"></field>
                                </tree>
                            </field>
                        </group>


                    <!--<button name="act_req_dk" type="object" string="Request" class="oe_highlight" attrs="{'invisible':['|','|',('x_status_cr','=','reject'),('x_flag_reqdk','=',True),('x_flag_appdk','=',True)]}"/>-->
                    <!--<button name="act_approve" type="object" string="Approve" class="oe_highlight" attrs="{'invisible':['|', '|','|',('x_status_cr','=','approve'), ('x_status_cr','=','reject'),('x_status_cr','=','done'),('x_is_salemanager', '=', False)]}"/>-->
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="list_waste_table">
            <field name="name">Waste Table</field>
            <field name="res_model">x.waste.table.precost</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>